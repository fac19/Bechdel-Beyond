{
	"name": "bechdel-beyond",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@material-ui/core": "^4.9.14",
		"@material-ui/icons": "^4.9.1",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.5.0",
		"@testing-library/user-event": "^7.2.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-router-dom": "^5.2.0",
		"react-scripts": "3.4.1",
		"react-test-renderer": "^16.13.1",
		"styled-components": "^5.1.0"
	},
	"scripts": {
		"start": "react-scripts -r @cypress/instrument-cra start",
		"build": "react-scripts build",
		"eject": "react-scripts eject",
		"test": "react-scripts test",
		"test:report": "react-scripts test --updateSnapshot --coverage --watchAll=false",
		"cy:open": "cypress open",
		"cy:report": "cypress run --headless",
		"cy:ci": "start-server-and-test start http://localhost:3000 cy:report"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"eslint",
			"pretty-quick --staged"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@cypress/code-coverage": "^3.8.1",
		"@cypress/instrument-cra": "^1.1.1",
		"codecov": "^3.7.0",
		"cypress": "^4.8.0",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"eslint": "^6.8.0",
		"eslint-config-airbnb": "^18.1.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-cypress": "^2.11.1",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.20.0",
		"eslint-plugin-react-hooks": "^2.5.0",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.4",
		"prettier": "^2.0.5",
		"pretty-quick": "^2.0.1",
		"react-axe": "^3.4.1",
		"start-server-and-test": "^1.11.0"
	},
	"jest": {
		"coveragePathIgnorePatterns": [
			"<rootDir>/src/serviceWorker.js",
			"<rootDir>cypress"
		]
	},
  "nyc": {
    "exclude": "<rootDir>/src/serviceWorker.js"
  }
}
